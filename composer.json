{
  "name": "dhii/service-tools",
  "description": "A set of tools for working with service providers",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Ukhanev",
      "email": "xedin.unknown@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Miguel Muscat",
      "email": "miguelmuscat93@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2 | ^8.0",
    "dhii/services-interface": "0.1.x-dev",
    "container-interop/service-provider": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 | ^9.0",
    "vimeo/psalm": "^4.0",
    "webmozart/path-util": "^2.3@stable",
    "slevomat/coding-standard": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Dhii\\Services\\Tools\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dhii\\Services\\Tools\\Test\\Func\\": "tests/functional"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-initial": "0.1.x-dev"
    }
  }
}
